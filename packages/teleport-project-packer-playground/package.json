  "version": "0.9.0",
  "description": "Teleport playground packer - use this package is you want to use all the project generators and all the publishers only from one package",
  "author": "teleportHQ",
  "license": "MIT",
  "homepage": "https://teleporthq.io/",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teleporthq/teleport-code-generators.git"
  },
  "bugs": {
    "url": "https://github.com/teleporthq/teleport-code-generators/issues"
