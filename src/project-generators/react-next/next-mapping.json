{
  "elements": {
    "navlink": {
      "type": "Link",
      "dependency": {
          "type": "library",
          "path": "next/link"
      },
      "attrs": {
        "href": { "type": "dynamic", "content": { "referenceType": "attr", "id": "transitionTo" } }
      },
      "children": [
        {
          "type": "a",
          "name": "link",
          "children": ["$children"]
        }
      ]
    }
  }
}