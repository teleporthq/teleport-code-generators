{
    "$schema": "https://raw.githubusercontent.com/teleporthq/teleport-code-generators/master/src/uidl-definitions/schemas/component.json",
    "name": "TabSelector",
    "stateDefinitions": {
      "activeTab": {
        "type": "number",
        "defaultValue": 0
      }
    },
    "propDefinitions": {
      "test": {
        "type": "string",
        "defaultValue": "123"
      }
    },
    "content": {
      "type": "container",
      "key": "string",
      "children": [
        {
          "type": "container",
          "attrs": {
            "data-test": {
              "type": "dynamic",
              "content": {
                "referenceType": "prop",
                "id":"test" 
              }
            }
          },
          "children": [
            {
              "type": "button",
              "children": ["Click"],
              "style": {
                "border": { "type": "static", "content": "1px solid black" },
                "padding": { "type": "static", "content": "5px 10px"} 
              },
              "events": {
                "onClick": [{
                  "type": "stateChange",
                  "modifies": "activeTab",
                  "newState": 0
                }]
              }
            },
            {
              "type": "button",
              "children": ["any"],
              "style": {
                "border": { "type": "static", "content": "1px solid black" },
                "padding": { "type": "static", "content": "5px 10px"} 
              },
              "events": {
                "onClick": [{
                  "type": "stateChange",
                  "modifies": "activeTab",
                  "newState": 1
                }]
              }
            },
            {
              "type": "button",
              "children": ["of"],
              "style": {
                "border": { "type": "static", "content": "1px solid black" },
                "padding": { "type": "static", "content": "5px 10px"} 
              },
              "events": {
                "onClick": [{
                  "type": "stateChange",
                  "modifies": "activeTab",
                  "newState": 2
                }]
              }
            },
            {
              "type": "button",
              "children": ["these"],
              "style": {
                "border": { "type": "static", "content": "1px solid black" },
                "padding": { "type": "static", "content": "5px 10px"} 
              },
              "events": {
                "onClick": [{
                  "type": "stateChange",
                  "modifies": "activeTab",
                  "newState": 3
                }]
              }
            }
          ]
        },
        {
          "type": "container",
          "style": {
            "width": { "type":"static", "content": 100 },
            "height": { "type":"static", "content": 50 },
            "display": { "type":"static", "content": "flex" },
            "alignItems": { "type":"static", "content": "flex-start" },
            "backgroundColor": { "type":"static", "content": "#e4f2f8" },
            "borderColor": { "type":"static", "content": "#0089be" },
            "borderWidth": { "type":"static", "content": 0 },
            "borderStyle": { "type":"static", "content": "solid" },
            "@media (max-width: 835px)": {
              "type": "nested-style",
              "content": {
                "width": { "type":"static", "content": 228 },
                "height": { "type":"static", "content": 154 }
              }
            },
            "@media (max-width: 640px)": {
              "type": "nested-style",
              "content": {
                "backgroundColor": { "type":"static", "content": "#e91e63" }
              }
            },
            "@media (max-width: 634px)": {
              "type": "nested-style",
              "content": {
                "height": { "type":"static", "content": 276 }
              }
            }
          },
          "children": [
            "Active tab: ",
            "$state.activeTab"
          ]
        },
        {
          "type": "state",
          "states": [
            {
              "value": 0,
              "content": {
                "type": "container",
                "children": [{
                  "type": "text",
                  "children": ["This is the first tab"]
                }]
              }
            },{
              "value": 1,
              "content": {
                "type": "text",
                "children": ["This is the second tab"]
              }
            },{
              "value": {
                "matchingCriteria": "any",
                "conditions": [{
                  "operand": 2,
                  "operation": "==="
                }]
              },
              "content": {
                "type": "text",
                "children": ["This is the third tab"]
              }
            },{
              "value": {
                "matchingCriteria": "all",
                "conditions": [{
                  "operand": 3,
                  "operation": ">="
                }, {
                  "operand": 5,
                  "operation": "<"
                }]
              },
              "content": {
                "type": "text",
                "children": ["This is the fourth tab"]
              }
            }
          ]
        }
      ]
    }
  }