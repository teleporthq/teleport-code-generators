{
  "$schema": "https://raw.githubusercontent.com/teleporthq/teleport-code-generators/master/src/uidl-definitions/schemas/project.json",
  "name": "myVueProject",
  "globals": {
    "settings": {
      "language": "en",
      "title": "Router Project"
    },
    "assets": [],
    "meta": []
  },
  "root": {
    "name": "App",
    "stateDefinitions": {
      "router": {
        "type": "router",
        "defaultValue": "home",
        "values": [
          {
            "value": "home",
            "meta": {
              "path": "/",
              "componentName": "HomePage",
              "fileName": "Home"
            }
          },
          {
            "value": "about",
            "meta": {
              "path": "/mask-url",
              "componentName": "AboutPage"
            }
          }
        ]
      }
    },
    "content": {
      "type": "states",
      "name": "router",
      "states": [
        {
          "value": "home",
          "content": {
            "type": "container",
            "name": "container",
            "children": [
              {
                "type": "button",
                "name": "button",
                "children": ["click-me"]
              },
              {
                "type": "CustomText",
                "name": "text",
                "attrs": {
                  "message": "random message"
                },
                "dependency": {
                  "type": "local"
                }
              },
              {
                "type": "navlink",
                "name": "navlink",
                "attrs": {
                  "transitionTo": "/mask-url"
                },
                "children": ["About page"]
              }
            ]
          }
        },
        {
          "value": "about",
          "content": {
            "type": "group",
            "name": "around",
            "children": [
              {
                "type": "text",
                "name": "aspan",
                "children": ["Who"]
              },
              {
                "type": "container",
                "name": "adiv",
                "children": ["are"]
              },
              {
                "type": "text",
                "name": "aspan2",
                "children": ["you?"],
                "attrs": {
                  "disabled": "true",
                  "data-test": "123"
                }
              },
              {
                "type": "Datepicker",
                "style": {
                  "margin": "10px"
                },
                "dependency": {
                  "type": "package",
                  "path": "vuejs-datepicker",
                  "version": "1.5.4",
                  "meta": {
                    "namedImport": false
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
  "components": {
    "CustomText": {
      "name": "CustomText",
      "propDefinitions": {
        "message": {
          "type": "string",
          "defaultValue": ""
        },
        "items": {
          "type": "array",
          "defaultValue": ["Hello", "is it me", "you're", "looking", "for?"]
        }
      },
      "content": {
        "type": "container",
        "style": {
          "color": "red"
        },
        "attrs": {
          "data-attr": "random"
        },
        "children": [
          {
            "type": "text",
            "children": ["$props.message"]
          },
          {
            "type": "dropdown",
            "attrs": {
              "options": ["angular", "react", "vue"],
              "disabled": true
            }
          },
          {
            "type": "list",
            "attrs": {
              "items": {
                "type": "dynamic",
                "content": {
                  "referenceType": "prop",
                  "id": "items"
                }
              }
            }
          }
        ]
      }
    },
    "Card": {
      "name": "Card",
      "propDefinitions": {
        "item": {
          "type": "string",
          "defaultValue": ""
        }
      },
      "content": {
        "type": "container",
        "name": "card",
        "style": {
          "margin": "10px",
          "padding": "10px",
          "border": "1px solid black"
        },
        "children": ["$props.children"]
      }
    },
    "Cards": {
      "name": "Cards",
      "propDefinitions": {
        "items": {
          "type": "array",
          "defaultValue": []
        }
      },
      "content": {
        "type": "container",
        "name": "cards-container",
        "style": {
          "width": "100%",
          "border": "1px solid red",
          "display": "flex"
        },
        "children": [{
          "type": "text",
          "name": "joker",
          "children": ["Last"]
        }],
        "repeat": {
          "content": {
            "type": "container",
            "name": "card-container",
            "style": {
              "backgroundColor": "royalblue",
              "color": "white",
              "margin": "10px",
              "border": "1px solid black"
            },
            "children": [{
              "type": "Card",
              "name": "card",
              "attrs": {
                "test": {
                  "type": "dynamic",
                  "content": {
                    "referenceType": "local",
                    "id":"index" 
                  }
                }
              },
              "children": ["$local.item"],
              "dependency": {
                "type": "local"
              }
            }]
          },
          "dataSource": "$props.items",
          "meta": {
            "useIndex": true
          }
        }
      }
    },
    "ExpandableArea": {
      "name": "ExpandableArea",
      "propDefinitions": {
        "title": {
          "type": "string",
          "defaultValue": "Hello"
        },
        "text": {
          "type": "string",
          "defaultValue": "Is it me you're looking for?"
        }
      },
      "stateDefinitions": {
        "isExpanded": {
          "type": "boolean",
          "defaultValue": false
        }
      },
      "content": {
        "type": "container",
        "name": "container",
        "children": [
          {
            "type": "text",
            "name": "title",
            "children": ["$props.title"]
          },
          {
            "type": "button",
            "name": "button",
            "children": [{
              "type": "state",
              "name": "isExpanded",
              "states": [
                {
                  "value": true,
                  "content": "Collapse"
                },
                {
                  "value": false,
                  "content": "Expand"
                }
              ]
            }],
            "events": {
              "click": [
                {
                  "type": "stateChange",
                  "modifies": "isExpanded",
                  "newState": "$toggle"
                }
              ]
            }
          },
          {
            "type": "state",
            "name": "isExpanded",
            "states": [
              {
                "value": true,
                "content": {
                  "type": "container",
                  "name": "expanded-area",
                  "children": ["$props.text"]
                }
              }
            ]
          }
        ]
      }
    },
    "Modal": {
      "name": "Modal",
      "stateDefinitions": {
        "isOpen": {
          "type": "boolean",
          "defaultValue": false
        }
      },
      "content": {
        "type": "group",
        "name": "container",
        "children": [
          {
            "type": "button",
            "name": "openmodal",
            "children": ["Show Popup"],
            "events": {
              "click": [{
                "type": "stateChange",
                "modifies": "isOpen",
                "newState": true
              }]
            }
          },
          {
            "type": "state",
            "name": "isOpen",
            "states": [
              {
                "value": true,
                "content": {
                  "type": "ModalWindow",
                  "name": "window",
                  "dependency": {
                    "type": "local"
                  },
                  "events": {
                    "onClose": [{
                      "type": "stateChange",
                      "modifies": "isOpen",
                      "newState": false
                    }]
                  }
                }
              }
            ]
          }
        ]
      }
    },
    "ModalWindow": {
      "name": "ModalWindow",
      "propDefinitions": {
        "message": {
          "type": "string",
          "defaultValue": "Hello"
        },
        "onClose": {
          "type": "func",
          "defaultValue": "() => {}"
        }
      },
      "content": {
        "type": "container",
        "name": "container",
        "children": [
          {
            "type": "text",
            "name": "text",
            "children": ["$props.message"]
          },
          {
            "type": "button",
            "name": "close",
            "children": ["Close me"],
            "events": {
              "click": [{
                "type": "propCall",
                "calls": "onClose"
              }]
            }
          }
        ]
      }
    }
  }
}