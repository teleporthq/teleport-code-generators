{
  "$schema": "https://raw.githubusercontent.com/teleporthq/teleport-code-generators/master/packages/teleport-generator-core/src/uidl-schemas/component.json",
  "name": "ComponentWithAttrProp",
  "propDefinitions": {
    "test": {
      "type": "string",
      "defaultValue": "123"
    }
  },
  "node": {
    "type": "element",
    "content": {
      "elementType": "container",
      "children": [{
        "type": "repeat",
        "content": {
          "node": {
            "type": "element",
            "content": {
              "elementType": "container",
              "children": [{
                "type": "element",
                "content": {
                  "elementType": "container",
                  "name": "card",
                  "attrs": {
                    "test": {
                      "type": "dynamic",
                      "content": {
                        "referenceType": "local",
                        "id": "index"
                      }
                    }
                  },
                  "children": [{
                    "type": "dynamic",
                    "content": { 
                      "referenceType": "local", 
                      "id": "item" 
                    }
                  }]
                }
              }]
            }
          },
          "dataSource":  { 
            "type": "dynamic",
            "content": { "referenceType": "prop", "id": "items" }
          },
          "meta": {
            "useIndex": true
          }
        }
      }]
    }
  }
}