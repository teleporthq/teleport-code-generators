{
  "name": "@teleporthq/teleport-project-plugin-next-styled-components",
  "version": "0.16.0",
  "description": "A plugin for generating a next project using styled-components",
  "author": "teleportHQ",
  "license": "MIT",
  "homepage": "https://teleporthq.io/",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teleporthq/teleport-code-generators.git"
  },
  "bugs": {
    "url": "https://github.com/teleporthq/teleport-code-generators/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run build:cjs & npm run build:esm",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm": "tsc -p tsconfig-esm.json"
  },
  "dependencies": {
    "@teleporthq/teleport-plugin-import-statements": "^0.16.0",
    "@teleporthq/teleport-plugin-react-styled-components": "^0.16.0",
    "@teleporthq/teleport-postprocessor-prettier-js": "^0.16.0",
    "@teleporthq/teleport-types": "^0.16.0",
    "magic-string": "^0.25.4"
  }
}
