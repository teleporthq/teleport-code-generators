{
  "name": "ComponentWithAttrProp",
  "propDefinitions": {
    "test": {
      "type": "string",
      "defaultValue": "123"
    },
    "content": {
      "type": "object",
      "defaultValue": {
        "heading": "Hello World"
      }
    }
  },
  "node": {
    "type": "element",
    "content": {
      "children": ["hello!"],
      "elementType": "container",
      "attrs": {
        "data-test": "$props.test",
        "data-static": "I am just a static string",
        "data-inner-value": "$props.content.heading"
      }
    }
  }
}