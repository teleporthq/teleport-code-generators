{
	"$schema": "https://docs.teleporthq.io/uidl-schema/v1/component.json",
	"name": "ComponentWithSingleDashCaseStyle",
	"propDefinitions": {
		"config": {
			"type": "object",
			"defaultValue": {
				"height": 32
			}
		},
		"backgroundColor": {
			"type": "string",
			"defaultValue": "blue"
		}
	},
	"node": {
		"type": "element",
		"content": {
			"elementType": "container",
			"style": {
				"alignSelf": {
					"type": "static",
					"content": "center"
				},
				"background-color": {
					"type": "dynamic",
					"content": {
						"referenceType": "prop",
						"id": "backgroundColor"
					}
				}
			}
		}
	}
}