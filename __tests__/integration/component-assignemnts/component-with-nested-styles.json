{
  "$schema": "https://raw.githubusercontent.com/teleporthq/teleport-code-generators/master/src/uidl-definitions/schemas/component.json",
  "name": "ComponentWithAttrProp",
  "propDefinitions": {
    "direction": {
      "type": "string",
      "defaultValue": "row"
    }
  },
  "content": {
    "type": "container",
    "style": {
      "flexDirection": {
        "type": "dynamic",
        "content": {
          "referenceType": "prop",
          "id": "direction"
        }
      },
      "@media (max-width: 835px)": {
        "type": "nested-style",
        "content": {
          "alignSelft": {
            "type": "static",
            "content": "inherit"
          }
        }
      },
      "alignSelft": {
        "type": "static",
        "content": "center"
      },
      "@media (max-width: 640px)": {
        "type": "nested-style",
          "content": {
            "alignSelft": {
              "type": "static",
              "content": "left"
            }
        }
      },
      "@media (max-width: 634px)": {
        "type": "nested-style",
        "content": {
          "alignSelft": {
            "type": "static",
            "content": "right"
          }
        }
      }
    },
    "children": []
  }
}